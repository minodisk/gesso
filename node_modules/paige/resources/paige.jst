<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <meta charset="UTF-8">
  <link rel="stylesheet" media="all" href="../resources/paige.css" />
</head>
<body>

<header>
  <h1><%= header %></h1>
  <% if(subheader != ""){ %>
    <em><%= subheader %></em>
  <% } %>
  <% if(include_index){ %>
  <h3>Source files:</h3>
  <% } %>
</header>

<% if(include_index){ %>
  <% for (var packageName in subfiles) { %>
<section>
  <h3><%= packageName %></h3>
  <ul>
  <% var files = subfiles[packageName]; %>
  <% for (var i = 0, len = files.length; i < len; i++) { %>
      <li><a href="<%= files[i].basename %>.html"><%= files[i].filename %></a></li>
  <% } %>
  </ul>
</section>
  <% } %>
<% } %>

<footer>
  <%= content_html %>
</footer>

</body>
</html>
